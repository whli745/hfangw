<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC  "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"  "http://getahead.org/dwr/dwr30.dtd">
<dwr>
	<allow>
		<create creator="spring" javascript="AreaManageDwr">
			<param name="beanName" value="areaManageDwr" />
		</create>
		<create creator="spring" javascript="PtSynthetizeAppDwr">
			<param name="beanName" value="ptSynthetizeAppDwr"/>
		</create> 
		<create creator="spring" javascript="OrganManagerDwr">
			<param name="beanName" value="organManagerDwr"/>
		</create>
		<create creator="spring" javascript="SysLocalManageDwr">
			<param name="beanName" value="sysLocalManageDwr"/>
		</create>
		
		<convert converter="bean" match="pt.xtgl.jcgl.pojo.SysArea" javascript="SysArea" />
		<convert converter="bean" match="pt.xtgl.jcgl.pojo.SysOrgan" />
		<convert converter="bean" match="pt.xtgl.jcgl.pojo.SysRole" />
		<convert converter="bean" match="dwr.vo.SysRoleVo" />
		<convert converter="bean" match="dwr.vo.SysMyUser" javascript="SysMyUser" />
		<convert converter="bean" match="pt.xtgl.jcgl.pojo.SysDict" />
		<convert converter="bean" match="pt.xtgl.jcgl.pojo.SysLocal" javascript="SysLocal" />
	</allow>
</dwr>
